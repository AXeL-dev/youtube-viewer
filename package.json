{
  "name": "youtube-viewer",
  "version": "0.4.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.8.3",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "4.0.0-alpha.39",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.47",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^12.2.0",
    "react-dom": "^16.13.1",
    "react-pull-to-refresh": "^1.1.2",
    "react-scripts": "^3.4.1",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "gulp compile:background-scripts && INLINE_RUNTIME_CHUNK=false react-scripts build",
    "package": "web-ext build --source-dir=build --artifacts-dir=. --overwrite-dest",
    "package:dev": "cd build && zip -r -FS ../youtube_viewer-dev.zip *",
    "release": "npm run build && npm run package",
    "release:dev": "npm run build && npm run package:dev",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.155",
    "@types/react-beautiful-dnd": "^11.0.5",
    "gulp": "^4.0.2",
    "gulp-file": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.8.0",
    "webextension-polyfill": "^0.6.0",
    "webextension-polyfill-ts": "^0.11.0",
    "yargs": "^15.3.1"
  }
}
